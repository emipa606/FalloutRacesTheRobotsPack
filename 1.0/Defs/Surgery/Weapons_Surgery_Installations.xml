    <anesthetize>false</anesthetize>
  </RecipeDef>
  <RecipeDef ParentName="SurgeryRobot">
    <defName>RemoveBodyParts</defName>
    <label>remove part</label>
    <description>Remove a body part.</description>
    <workerClass>Recipe_RemoveBodyPart</workerClass>
    <jobString>Removing body part.</jobString>
    <workAmount>2000</workAmount>
    <surgerySuccessChanceFactor>2</surgerySuccessChanceFactor>
  </RecipeDef>
  <RecipeDef ParentName="SurgeryRobot">
    <defName>EuthanizeBot</defName>
    <label>euthanize by cut</label>
    <description>Euthanize a creature by cutting.</description>
    <workerClass>Recipe_ExecuteByCut</workerClass>
    <jobString>Cutting oil line.</jobString>
    <workAmount>500</workAmount>
    <hideBodyPartNames>true</hideBodyPartNames>
    <isViolation>true</isViolation>
    <targetsBodyPart>false</targetsBodyPart>
    <surgerySuccessChanceFactor>5</surgerySuccessChanceFactor>
  </RecipeDef>
  <RecipeDef ParentName="SurgeryRobot">
    <defName>InstallMountedFlamethrower</defName>
    <label>install mounted flamethrower</label>
    <description>Install a mounted flamethrower.</description>
    <workerClass>Recipe_InstallArtificialBodyPart</workerClass>
    <jobString>Installing mounted flamethrower.</jobString>
    <workAmount>1000</workAmount>
    <surgerySuccessChanceFactor>2000</surgerySuccessChanceFactor>
    <skillRequirements>
      <Crafting>3</Crafting>
    </skillRequirements>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>MountedFlamethrower</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>MountedFlamethrower</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>WeaponMount</li>
    </appliedOnFixedBodyParts>
    <addsHediff>MountedFlamethrower</addsHediff>
    <recipeUsers>
      <li>Human</li>
    </recipeUsers>
  </RecipeDef>
